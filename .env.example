# Environment variables for development
SA_PASSWORD=YourStrong@Passw0rd123  # CHANGE THIS PASSWORD!
ACCEPT_EULA=Y
MSSQL_PID=Express
DOTNET_ENVIRONMENT=Development
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://+:80

# Database connection (UPDATE PASSWORD!)
ConnectionStrings__DefaultConnection=Server=localhost;Database=YourDatabase;User Id=sa;Password=YourStrong@Passw0rd123;TrustServerCertificate=true;

# Custom application variables
CUSTOM_VAR_1=DevelopmentValue1
CUSTOM_VAR_2=DevelopmentValue2

# CI/CD Configuration
ENABLE_CI_CD=true
GITHUB_REPO=your-username/your-repo
GITHUB_BRANCH=main
GITHUB_DEPLOY_KEY=-----BEGIN OPENSSH PRIVATE KEY-----...-----END OPENSSH PRIVATE KEY-----
POLL_INTERVAL=60
BUILD_SCRIPT=deploy.sh
ENABLE_AUTO_BUILD=true

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft=Warning
Logging__LogLevel__System=Warning
